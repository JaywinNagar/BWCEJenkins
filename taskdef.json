{
    "family": "launch-test-app",
    "containerDefinitions": [
        {
            "image": "750037626691.dkr.ecr.eu-central-1.amazonaws.com/http-app:latest",
            "name": "bwce-test-app",
            "cpu": 10,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ],
            "environment": [
                {
                    "name": "BW_LOGLEVEL",
                    "value": "DEBUG"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "bwce-app-log",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "bwce"
                }
	    }
        }
    ]
}
